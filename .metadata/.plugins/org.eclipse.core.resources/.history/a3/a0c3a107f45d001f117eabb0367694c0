<script type="text/javascript">
function f(x,y) { 
	var z = x + y;
	
	if(z < 10) {
		return; // 함수를 벗어나라
	}
	return z; // 반환
}

let a = f(2, 1);
let b = f(5, 7);
let c = f(10.8, 5.4);
