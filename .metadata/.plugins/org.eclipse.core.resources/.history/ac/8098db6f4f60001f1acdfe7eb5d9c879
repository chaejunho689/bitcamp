$(function() {
	$('#join').submit(function(){
		let user_id = $('input[name="user_id"]').val();
		if(!user_id) {
			alert("아이디를 입력하세요.");
			$('input[name="user_id"]').focus();
			
			return false;
		}
		
		let user_pw = $('input[name="user_id"]').val();
				if(!user_id) {
					alert("아이디를 입력하세요.");
					$('input[name="user_id"]').focus();
					
					return false;
				}
				
		return false;
	});	
});